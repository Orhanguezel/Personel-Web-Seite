{
    "info": {
      "name": "User Registration Collection",
      "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [
      {
        "name": "Register Users",
        "event": [
          {
            "listen": "test",
            "script": {
              "exec": [
                "const users = pm.collectionVariables.get('users');",
                "const userArray = JSON.parse(users);",
                "userArray.forEach(user => {",
                "  pm.sendRequest({",
                "    url: 'http://localhost:5000/api/users/register',",
                "    method: 'POST',",
                "    header: { 'Content-Type': 'application/json' },",
                "    body: {",
                "      mode: 'raw',",
                "      raw: JSON.stringify(user)",
                "    }",
                "  }, (err, res) => {",
                "    console.log(res);",
                "  });",
                "});"
              ]
            }
          }
        ],
        "request": {
          "method": "POST",
          "header": [
            {
              "key": "Content-Type",
              "value": "application/json"
            }
          ],
          "body": {
            "mode": "raw",
            "raw": ""
          },
          "url": {
            "raw": "http://localhost:5000/api/users/register",
            "protocol": "http",
            "host": ["localhost"],
            "port": "5000",
            "path": ["api", "users", "register"]
          }
        }
      }
    ],
    "variable": [
      {
        "key": "users",
        "value": "[{\"username\":\"admin\",\"email\":\"admin@example.com\",\"password\":\"adminpassword\",\"role\":\"admin\"},{\"username\":\"user1\",\"email\":\"user1@example.com\",\"password\":\"userpassword1\",\"role\":\"user\"},{\"username\":\"user2\",\"email\":\"user2@example.com\",\"password\":\"userpassword2\",\"role\":\"user\"},{\"username\":\"guest1\",\"email\":\"guest1@example.com\",\"password\":\"guestpassword1\",\"role\":\"guest\"},{\"username\":\"guest2\",\"email\":\"guest2@example.com\",\"password\":\"guestpassword2\",\"role\":\"guest\"}]"
      }
    ]
  }
  